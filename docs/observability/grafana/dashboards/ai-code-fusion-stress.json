{
  "uid": "ai-code-fusion-stress",
  "title": "AI Code Fusion - Stress Metrics",
  "tags": ["ai-code-fusion", "performance", "stress"],
  "timezone": "browser",
  "schemaVersion": 41,
  "version": 2,
  "refresh": "30s",
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Latest Publish Age (min)",
      "datasource": {
        "type": "prometheus",
        "uid": "ee44ub862vq4gc"
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "(time() - max(ai_code_fusion_stress_publish_timestamp_seconds)) / 60",
          "instant": true,
          "legendFormat": "age_min",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "m",
          "decimals": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 30
              },
              {
                "color": "red",
                "value": 120
              }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Scenarios Reporting",
      "datasource": {
        "type": "prometheus",
        "uid": "ee44ub862vq4gc"
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "expr": "count(max by (scenario) (ai_code_fusion_stress_sample_count))",
          "instant": true,
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "decimals": 0
        },
        "overrides": []
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Latest p95 Worst Case (ms)",
      "datasource": {
        "type": "prometheus",
        "uid": "ee44ub862vq4gc"
      },
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "max(max by (scenario, percentile) (ai_code_fusion_stress_latency_ms{percentile=\"p95\"}))",
          "instant": true,
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 2
        },
        "overrides": []
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Latency by Scenario and Percentile",
      "datasource": {
        "type": "prometheus",
        "uid": "ee44ub862vq4gc"
      },
      "gridPos": {
        "h": 10,
        "w": 12,
        "x": 0,
        "y": 6
      },
      "targets": [
        {
          "expr": "max by (scenario, percentile) (ai_code_fusion_stress_latency_ms)",
          "legendFormat": "{{scenario}} {{percentile}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 2
        },
        "overrides": []
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Sample Count by Scenario",
      "datasource": {
        "type": "prometheus",
        "uid": "ee44ub862vq4gc"
      },
      "gridPos": {
        "h": 10,
        "w": 12,
        "x": 12,
        "y": 6
      },
      "targets": [
        {
          "expr": "max by (scenario) (ai_code_fusion_stress_sample_count)",
          "legendFormat": "{{scenario}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        },
        "overrides": []
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "File Count and Iterations",
      "datasource": {
        "type": "prometheus",
        "uid": "ee44ub862vq4gc"
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 16
      },
      "targets": [
        {
          "expr": "max by (scenario) (ai_code_fusion_stress_file_count)",
          "legendFormat": "{{scenario}} file_count",
          "refId": "A"
        },
        {
          "expr": "max by (scenario) (ai_code_fusion_stress_iterations)",
          "legendFormat": "{{scenario}} iterations",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        },
        "overrides": []
      }
    }
  ]
}
